<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_Special_Sym_and_not_start_with_Digit</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX( Flight_Number__c , &apos;^[A-Za-z]+[-][0-9]*&apos;))</errorConditionFormula>
    <errorDisplayField>Flight_Number__c</errorDisplayField>
    <errorMessage>Flight Number can not start with Digit And No Special Symbol except - is allowed.</errorMessage>
</ValidationRule>

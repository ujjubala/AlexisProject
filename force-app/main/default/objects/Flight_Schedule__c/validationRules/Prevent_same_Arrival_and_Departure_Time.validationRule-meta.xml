<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_same_Arrival_and_Departure_Time</fullName>
    <active>true</active>
    <errorConditionFormula>TIMEVALUE( Arrival_Date_Time__c )  = 
 TIMEVALUE( Departure_Date_Time__c )</errorConditionFormula>
    <errorMessage>Arrival and Departure time cant be same</errorMessage>
</ValidationRule>

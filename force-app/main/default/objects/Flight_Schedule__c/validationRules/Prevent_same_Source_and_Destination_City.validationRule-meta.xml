<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_same_Source_and_Destination_City</fullName>
    <active>true</active>
    <errorConditionFormula>TEXT( Source_Location_City__c )   = 
   TEXT( Destination_City__c )</errorConditionFormula>
    <errorMessage>Same Source and Destination City..</errorMessage>
</ValidationRule>
